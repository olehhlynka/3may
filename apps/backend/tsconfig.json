{
  "compilerOptions": {
    "lib": ["ES2022"],
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "target": "ES2022",
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "exclude": ["node_modules", ".esbuild", ".serverless"]
}
